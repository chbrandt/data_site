{
  "$id": "gmap_package.schema",

  "title": "GMAP package schema",
  "description": "GMAP data packages are summarized by a set of metadata, defined here.",

  "type": "object",

  "properties": {
    "meta.json": { "$ref" : "gmap_package_meta.schema.json#" },
    "document/": { "$ref" : "gmap_package_document.schema.json#" },
    "vector/": { "$ref" : "gmap_package_vector.schema.json#" },
    "raster/": { "$ref" : "gmap_package_raster.schema.json#" }
  },

  "required": [
    "meta.json"
  ],
}
